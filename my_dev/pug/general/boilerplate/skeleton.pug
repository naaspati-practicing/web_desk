doctype html
html
 head
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  
  block head
   title= title || ""
   meta(name="description", content=description || "")  
   meta(name="author", content=author || "")  
 
   if scripts
    each js in scripts
     script(src=js, defer)
	
   if stylesheets
    each css in stylesheets
     link(rel="stylesheet", href=css)
   
   if(ie9)
     include ie9_support.html
 
 body
  if(old_browser_warning)
   include old_browser_warning.html
  if(google_analytics)
   script
    include google_analytics.js
  |
  |
  block body